{
  "stepDefinitions": [
    "cypress/e2e/steps/[filepath].steps.js",
    "cypress/e2e/common/*.js"
  ],
  "messages": {
    "enabled": true,
    "output": "cypress/reports/cucumber/cucumber-messages.ndjson"
  },
  "json": {
    "enabled": false,
    "output": "cypress/reports/cucumber/cucumber-report.json"
  },
  "html": {
    "enabled": true,
    "output": "cypress/reports/cucumber/basic-report/cucumber-report.html"
  },
  "omitFiltered": false,
  "filterSpecs": true,
  "cucumber.features": [
    "src/test/**/*.feature",
    "features/**/*.feature",
    "tests/**/*.feature",
    "*specs*/**/.feature"
  ],
  "cucumber.glue": [
    "src/test/**/*.java",
    "features/**/*.js",
    "features/**/*.jsx",
    "features/**/*.ts",
    "features/**/*.tsx",
    "features/**/*.php",
    "features/**/*.py",
    "tests/**/*.py",
    "tests/**/*.rs",
    "features/**/*.rs",
    "features/**/*.rb",
    "*specs*/**/.cs"
  ]
}
